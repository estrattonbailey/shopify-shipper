{
  "name": "@barrelny/scaffold",
  "version": "0.0.1",
  "description": "A shared component scaffold",
  "author": "Max Rolon <max.rolon@barrelny.com>",
  "main": "./dist/main.js",
  "engines": {
    "node": "^6.0.0"
  },
  "scripts": {
    "test": "node test/node-index.js",
    "browserify": "node browserify.js",
    "jscs:watch": "onchange './src/**/*.js' -- npm run jscs",
    "jscs": "jscs src/*.js src/**/*.js > jscs.js || true",
    "jshint:watch": "onchange './src/**/*.js' -- npm run jshint",
    "jshint": "jshint src/*.js src/**/*.js > jshint.js || true",
    "browserstack": "node_modules/.bin/browserstack-runner"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "6.6.0",
    "babelify": "^7.3.0",
    "browserify": "^12.0.1",
    "browserstack-runner": "0.4.3",
    "jscs": "^3.0.4",
    "jshint": "^2.9.1",
    "onchange": "^2.4.0",
    "qunit": "~0.5.1",
    "watchify": "^3.7.0"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "nanoajax": "^0.4.3"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:barrel/shared-component-scaffold.git"
  },
  "license": "ISC"
}
